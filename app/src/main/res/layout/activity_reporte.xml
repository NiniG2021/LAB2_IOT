<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Reporte">

    <LinearLayout
        android:layout_width="320dp"
        android:layout_height="600dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="65dp"
        android:layout_marginEnd="66dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="33dp"
            android:text="Computadoras:"
            android:textSize="22dp"/>

        <TextView
            android:id="@+id/reporte_computadoras"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="No se registran"
            android:textSize="22dp"/>

        <TextView
            android:id="@+id/reporte_cp_2"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            android:textSize="22dp"/>

        <TextView
            android:id="@+id/reporte_teclado"
            android:layout_width="match_parent"
            android:layout_height="32dp"
            android:text="Teclado:"
            android:textSize="22dp"/>

        <TextView
            android:id="@+id/reporte_monitor"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:text="Monitor:"
            android:textSize="22dp"/>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>